####Q6) Write a c program to print ASCII value of a character

#include <iostream>
using namespace std;
int main()
{
    char c = 'A';
    cout << "The ASCII value of " << c << " is " << int(c);
    return 0;
}

####Q7)  Write a Progam to check if a given string palindrome?

#include <iostream>
#include<bits/stdc++.h>
#include<string.h>
using namespace std;

int main()
{
   char str[100];
   int i,length=0,flag=0;
   gets(str);
   length = strlen(str);
   for(i=0;i<=length/2;i++){
       if(str[i]==str[length-i-1]);
       flag++;
   }
   if(flag == i){
       cout<<"String entered is a palindrome";
   }
   else{
      cout<<"String entered is not a palindrome";
   }

    return 0;
}

####Q8)  Write a c program to check given number is palindrome number or not.

#include <iostream>

using namespace std;

int main()
{
    int n, num, digit, rev=0;
    cin>>num;
    n=num;
    do{
        digit = num%10;
        rev = (rev*10) + digit;
        num = num/10;
    }while(num!=0);
    if(n==rev){
        cout<<"Number is palindrome";
    }
    else{
        cout<<"Number is not palindrome";
    }

    return 0;
}
